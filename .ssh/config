Host *
    User harrison.heck
    IdentityFile ~/.ssh/id_rsa
    IdentityFile ~/.ssh/linio-live
    UseKeychain yes
    AddKeysToAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 600

Host *.linio-staging.com
    User harrison.heck

Host *.linio-development.com
    User harrison.heck

Host *.linio.com
    User harrison.heck
